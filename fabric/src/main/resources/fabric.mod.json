{
  "schemaVersion": 1,
  "id": "mountainous",
  "version": "${version}",
  "name": "Mountainous",
  "description": "A mod that expands the mountains and plains of Minecraft",
  "authors": [
    "Skyternity Team"
  ],
  "license": "Apache-2.0",
  "icon": "assets/mountainous/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [{
      "adapter": "kotlin",
      "value": "com.skyternity.mountainous.fabric.MountainousFabric"
    }],
    "client": [{
      "adapter": "kotlin",
      "value": "com.skyternity.mountainous.fabric.MountainousFabricClient"
    }]
  },
  "depends": {
    "fabricloader": ">=0.11.1",
    "fabric": "*",
    "architectury": ">=1.1.55",
    "fabric-language-kotlin": ">=1.6.0+kotlin.1.5.0"
  }
}